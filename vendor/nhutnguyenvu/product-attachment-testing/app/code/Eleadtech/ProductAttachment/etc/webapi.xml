<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/attachment/sku/:sku">
        <service class="Eleadtech\ProductAttachment\Api\AttachmentsInterface" method="getAttachments"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/attachment/skus">
        <service class="Eleadtech\ProductAttachment\Api\AttachmentsInterface" method="getAttachmentList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/attachment/carts/:cartId">
        <service class="Eleadtech\ProductAttachment\Api\AttachmentsInterface" method="getAllItemIdsAttachments"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
